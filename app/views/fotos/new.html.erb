<%content_for :head do%>
	<%=stylesheet_link_tag "foto"%>
<%end%>

<div id="Boxcenter">
	<%=render :file => "/layouts/_top.html.erb"%>
	
	<div id="Midboxfoto">
		<img src="/images/tit_fotos.png" width="112" height="59"  style="margin:20px 0 10px 30px; float:left;"/>
	</div>
	
	<div id="Bottomboxfotos">

		<div id="Boxlistafotos">

		<% form_for ([@album, @foto], :html => { :multipart => :true }) do |f| %>
		<h1>Fazer upload de foto</h1>
		<p>
		<%= f.label :legenda %><br />
		<%= f.text_field :legenda %>
		</p>
		<p>
		<%= f.label :image %><br />
		<%= f.file_field :image %>
		</p>
		<p><%= f.submit %></p>
		<% end %>
		</div>
	</div>
</div>